<%= render "shared/headers" %>
<main>
  <%= form_with model: @inquiry, local: true do |f| %>
    <%= f.text_field :name, placeholder:"名前" %>
    <%= f.text_field :mail, placeholder:"メールアドレス" %>
    <%= f.text_field :message, placeholder:"メッセージ" %>
    <%= f.submit "投稿する" %>
  <% end %>
</main>

<%= render "shared/footers" %>